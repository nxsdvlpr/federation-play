{
  "name": "federation",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "codegen": "codegen",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "codegen": "workspace:*",
    "config": "workspace:^1.0.0",
    "eslint": "^8.27.0",
    "eslint-plugin-import": "^2.26.0",
    "esm": "^3.2.25",
    "nxsutil": "workspace:1.0.0",
    "plop": "^3.1.1",
    "prettier": "latest",
    "tsconfig": "workspace:*",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.13.4",
  "dependencies": {
    "@nxs/helper": "workspace:^1.0.0"
  }
}